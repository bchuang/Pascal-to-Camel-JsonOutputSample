</div>
<b><font size="20px" color="red">Back Data :</font></b>
<div id="divResult"></div>

<b><font size="20px" color="red">Back Data(Api) :</font></b>
<div id="divApiResult"></div>

<script>
    var data = {
        rooms: [
            {
                no: 1,
                roomId: "1",
                roomName: "Name_1",
                users: null,
            },
            {
                no: 2,
                roomId: "2",
                roomName: "Name_2",
                users: [
                    { empId: "empId_1", name: "userName_1" },
                    { empId: "empId_2", name: "userName_2" }
                ]
            }
        ]
    };

    $.post("/Home/Test", data, function (result) {
        $("#divResult").html(result);
    });

    $.post("/api/Room", data, function (result) {
        $("#divApiResult").html(JSON.stringify(result));
    });
</script>